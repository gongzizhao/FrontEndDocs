function fibonacci(n){
	let ori = [0,1];
	if (n < 2) {
		return ori[n];
	};
	let fiboOne = 1,fiboTwo = 0,fiboSum = 0;
	for (let i = 2; i < n; i++) {
		fiboSum = fiboOne + fiboTwo;
		fiboTwo = fiboOne;
		fiboOne = fiboSum;
	}
	return fiboSum;
}

var fib=function(n){
    function fibIml(n,a,b){
        if(n===0) return a
        return fibIml(n-1,b,a+b)
    }
    return fibIml(n,0,1)
}

console.log(fibonacci(5))
console.log(fib(5))
